arch=arm
baudrate=115200
fdtaddr=0x18000000
initrd_addr=0x13000000
fdt_high=0xffffffff
vendor=technologic
soc=imx6
bootdelay=1
snappy_cmdline=net.ifnames=0 ro
snappy_mode=
uenvcmd=echo ENVCMD; run snappy_boot;
snappy_boot=if test "${snap_mode}" = "try"; then setenv snap_mode "trying"; saveenv; if test "${snap_try_core}" != ""; then setenv snap_core "${snap_try_core}"; fi; if test "${snap_try_kernel}" != ""; then setenv snap_kernel "${snap_try_kernel}"; fi; elif test "${snap_mode}" = "trying"; then setenv snap_mode ""; saveenv; fi; run mmcboot;
mmcboot=load mmc ${mmcdev}:${mmcpart} ${loadaddr} /ts4900-fpga.bin; ice40 ${loadaddr} ${filesize}; bbdetect; if load mmc ${mmcdev}:${mmcpart} ${fdtaddr} ${snap_kernel}/dtbs/imx6${cpu}-ts4900-${baseboardid}.dtb; then echo $baseboardid detected; else echo Booting default device tree; load mmc ${mmcdev}:${mmcpart} ${fdtaddr} ${snap_kernel}/dtbs/imx6${cpu}-ts${model}.dtb; fi; load mmc ${mmcdev}:${mmcpart} ${initrd_addr} ${snap_kernel}/initrd.img; env set initrd_size ${filesize}; load mmc ${mmcdev}:${mmcpart} ${loadaddr} ${snap_kernel}/kernel.img; env set bootargs console=ttymxc0,115200 root=/dev/mmcblk${mmcblk}p2 snap_core=${snap_core} snap_kernel=${snap_kernel} ${snappy_cmdline}; env print bootargs; bootz ${loadaddr} ${initrd_addr}:${initrd_size} ${fdtaddr};
bootcmd=if test ${jpsdboot} = "on"; then env set mmcdev 1; else env set mmcdev 2; fi; run importbootenv;
mmcpart=1
loadaddr=0x12000000
importbootenv=echo Importing environment from mmc ${mmcdev}...; load mmc ${mmcdev}:${mmcpart} ${loadaddr} /uboot.conf; env import -t -r $loadaddr $filesize;
stderr=serial
stdin=serial
stdout=serial
